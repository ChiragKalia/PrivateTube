{
    "manifest_version": 2,
    "name": "Private Tube",
    "description": "Quick Launch Private Tube",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["popup.js"]
      },
    "permissions":  ["activetab"]
}